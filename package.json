{
    "name": "top-builds",
    "version": "1.0.0",
    "description": "",
    "main": "public/index.html",
    "author": "",
    "license": "ISC",
    "scripts": {
        "build": "run-p -l build:*",
        "build:front": "parcel build src/index.html --out-dir public --public-url ./",
        "build:cache": "tsc",
        "cache": "node -r dotenv/config ./dist/index.js",
        "lint": "run-p -l lint:*",
        "lint:cache": "tslint -c tslint.json -p tsconfig.json",
        "dev": "run-p -l dev:*",
        "dev:front": "run-p -l watch:front host",
        "dev:cache": "tsc --watch --preserveWatchOutput",
        "watch:front": "parcel watch src/index.html --out-dir dist",
        "host": "live-server --port=8154 --mount=/data:./public/data ./dist"
    },
    "dependencies": {
        "@websanova/vue-store": "^1.0.5-beta",
        "bottleneck": "^2.19.5",
        "dotenv": "^8.0.0",
        "got": "^10.6.0",
        "lodash": "^4.17.20",
        "lodash-es": "^4.17.15",
        "petal.less": "^0.12.3",
        "vue": "^2.6.11",
        "vue-hot-reload-api": "^2.3.4",
        "vue-resource": "^1.5.1"
    },
    "devDependencies": {
        "@types/got": "^9.6.9",
        "@types/lodash": "^4.14.149",
        "@types/lodash-es": "^4.17.3",
        "@vue/component-compiler-utils": "^3.1.1",
        "less": "^3.11.1",
        "live-server": "^1.2.1",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2",
        "vue-template-compiler": "^2.6.11"
    }
}
